    "gantt",
    "project-management",
    "建設業",
    "工事管理",
    "工程管理"
  ],
  "author": "PLark-droid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PLark-droid/construction-lark.git"
  },
  "bugs": {
    "url": "https://github.com/PLark-droid/construction-lark/issues"
  },
  "homepage": "https://github.com/PLark-droid/construction-lark#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
